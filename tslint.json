{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "XXX_extends": [
        "tslint-no-circular-imports"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": {
            "options": "never-prefix"
        },
        "variable-name": {
            "options": "allow-leading-underscore"
        },
        "max-classes-per-file": false,
        "no-console": false,
        "no-namespace": false,
        "deprecation": true
    },
    "rulesDirectory": []
}